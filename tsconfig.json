{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "removeComments": true,
        "strictNullChecks": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "baseUrl": ".",
        "types": ["node"]
      }
}